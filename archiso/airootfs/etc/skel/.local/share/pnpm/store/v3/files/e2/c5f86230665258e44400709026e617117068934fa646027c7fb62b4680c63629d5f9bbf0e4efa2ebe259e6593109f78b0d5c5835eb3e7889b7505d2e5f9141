import { OptionType } from "../../utils/types";
declare const enum MentionOptions {
    DISABLED = 0,
    ENABLED = 1,
    NO_REPLY_MENTION_PLUGIN = 2
}
declare const _default: {
    name: string;
    authors: {
        name: string;
        id: bigint;
    }[];
    description: string;
    settings: import("../../utils/types").DefinedSettings<{
        shouldMention: {
            type: OptionType.SELECT;
            description: string;
            options: ({
                label: string;
                value: MentionOptions;
                default: true;
            } | {
                label: string;
                value: MentionOptions;
                default?: undefined;
            })[];
        };
    }, import("../../utils/types").SettingsChecks<{
        shouldMention: {
            type: OptionType.SELECT;
            description: string;
            options: ({
                label: string;
                value: MentionOptions;
                default: true;
            } | {
                label: string;
                value: MentionOptions;
                default?: undefined;
            })[];
        };
    }>, {}>;
    start(): void;
    stop(): void;
} & Record<string, any>;
export default _default;
