/// <reference types="react" />
import { OptionType } from "../../utils/types";
declare const enum IndicatorType {
    SERVER = 1,
    FRIEND = 2,
    BOTH = 3
}
declare function handlePresenceUpdate(): void;
declare function handleGuildUpdate(): void;
declare const _default: {
    name: string;
    description: string;
    authors: {
        name: string;
        id: bigint;
    }[];
    dependencies: string[];
    options: {
        mode: {
            description: string;
            type: OptionType.SELECT;
            options: ({
                label: string;
                value: IndicatorType;
                default: true;
            } | {
                label: string;
                value: IndicatorType;
                default?: undefined;
            })[];
        };
    };
    renderIndicator: () => JSX.Element;
    flux: {
        PRESENCE_UPDATES: typeof handlePresenceUpdate;
        GUILD_CREATE: typeof handleGuildUpdate;
        GUILD_DELETE: typeof handleGuildUpdate;
    };
    start(): void;
    stop(): void;
} & Record<string, any>;
export default _default;
